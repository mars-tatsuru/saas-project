{
  "cSpell.words": [
    "apexchart",
    "apexcharts",
    "crawlurl",
    "Dagre",
    "swapy",
    "xaxis"
  ],

  // prettier
  "prettier.enable": false,
  "editor.formatOnSave": false,

  // setting save
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },

  // eslint
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ]
}
